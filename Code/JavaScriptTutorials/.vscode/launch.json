    "version": "0.2.0",
    "configurations": [


    
        {
            "type": "node",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program Tutorial",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/TutorialASA.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program Create",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/CreateNewAccounts.js"
        }

    ]
}